{
    "config_name": "0_binary",
    "data": {
        "path_to_images": "path/to/images",
        "path_to_train_json": "path/to/train.json",
        "path_to_val_json": "path/to/val.json",
        "path_to_test_json": "path/to/test.json",
        "path_to_test_result_output_folder": "test_results/",
        "path_to_pytorch_pretrained_model": "weights/resnet34-b627a593.pth"
    },
    "classifier": {
        "resnet_layers": [3, 4, 6, 3],

        "keys_outputs": ['coco_attrib1','coco_attrib2','coco_attrib3','coco_attrib4'],
        "num_classes": [1, 1, 1, 1]
    },
    "train_config": {
        "weights_path": "weights/",
        "start_epoch": 0,
        "epochs": 180,
        "batch_size": 134,
        "workers": 4,
        "learning_rate": 0.1,
        "momentum": 0.9,
        "weight_decay": 1e-4,
        "print_freq": 10,
        "resume": null,
        "use_criterion_weights": true,
        "use_pytorch_pretrained_model": true,
        "resize_h": 240,
        "resize_w": 120,
        "transform_train": {
            "use_motion_blur": true,
            "use_planckian_jitter": true,
            "use_random_affine": false
        }
    }
}
